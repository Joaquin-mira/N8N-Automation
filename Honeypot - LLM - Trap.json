      "id": "9e97ea54-92b0-4d5a-87a0-c05e8fc9d8fa",
      "name": "Detección de Amenaza"
    },
    {
      "parameters": {
        "jsCode": "const textoCrudo = $input.first().json.output;\ntry {\n  const objetoLimpio = JSON.parse(textoCrudo);\n\n    return {\n  ataque: objetoLimpio.attack_type,\n  solucion: objetoLimpio.solution,\n  nivel_riesgo: objetoLimpio.risk_level\n};\n  } catch (error) {\n    return {\n      error: \"No se pudo procesar el JSON\",\n      detalle: error.message\n    }\n  }\n  "
      },
